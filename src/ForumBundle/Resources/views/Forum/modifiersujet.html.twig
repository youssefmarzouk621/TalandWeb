{% extends "default/LayoutFront.html.twig" %}
{% block section %}

    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-lg-8">
                <div class="login-content">
                    <h3>Add Subject</h3>
                    <!-- Login Form -->
                    {{ form_start(modifform,{'attr':{'novalidate':'novalidate'}}) }}
                    <div class="form-group">
                        <label for="inputAddress">Description</label>
                        {{ form_widget(modifform.descriptionF, { 'attr': {'class': 'form-control'} }) }}
                    </div>


                    {{ form_row(modifform.Captcha) }}
                    {{ form_widget(modifform.Add, { 'attr': {'class': 'btn oneMusic-btn mt-30'} }) }}



                    {{ form_end(modifform) }}
                </div>
            </div>
        </div>
    </div>

{% endblock %}





