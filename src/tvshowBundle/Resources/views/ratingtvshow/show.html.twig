{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ratingtvshow</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ratingtvshow.id }}</td>
            </tr>
            <tr>
                <th>Idtvshow</th>
                <td>{{ ratingtvshow.idtvshow }}</td>
            </tr>
            <tr>
                <th>Idu</th>
                <td>{{ ratingtvshow.idu }}</td>
            </tr>
            <tr>
                <th>Rate</th>
                <td>{{ ratingtvshow.rate }}</td>
