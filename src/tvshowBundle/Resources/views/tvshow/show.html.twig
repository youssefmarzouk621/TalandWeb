        function supcom(id) {
            $.ajax({
                url: 'http://localhost/taland/web/app_dev.php/Tvshow/suppcommentaire/' + id,
                success: function (data) {
                    if (data=="del"){
                        window.location.replace(window.location.href);
                    }
                },
                error: function (xhr, textStatus, errorThrown) {
                    console.log("Ajax request failed.");
                }
            });
        }
        function changeColorwhite(){
            document.body.style.backgroundColor = 'white';

            var x = document.getElementById("lampekahla");
            x.style.display = "block";
            var v = document.getElementById("lampe");
            v.style.display = "none";
        }
            $('.rating ul li').on('dblclick', function() {
                    $("#tes3").trigger('dblclick');
            })

                $('.rating ul li').on('click', function() {

            let li = $(this),
                ul = li.parent(),
                rating = ul.parent(),
                last = ul.find('.current');
                    $("#test3").val(li.index()+1);
               if($("#test3").val() != 0){
               $("#tes3").trigger('click');
               }


            if(!rating.hasClass('animate-left') && !rating.hasClass('animate-right')) {

                last.removeClass('current');

                ul.children('li').each(function() {
                    let current = $(this);
                    current.toggleClass('active', li.index() > current.index());
                });

                rating.addClass(li.index() > last.index() ? 'animate-right' : 'animate-left');
                rating.css({
                    '--x': li.position().left + 'px'
                });
                li.addClass('move-to');
                last.addClass('move-from');

                setTimeout(() => {
                    li.addClass('current');
                    li.removeClass('move-to');
                    last.removeClass('move-from');
                    rating.removeClass('animate-left animate-right');
                }, 800);

            }

        })

    </script>

